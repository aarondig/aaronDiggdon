ssName=\"project-content-container open\">\n        <motion.div\n          className=\"project-content\"\n          layoutId={`project-container-${id}`}\n        >\n          <motion.div\n            className=\"project-image-container\"\n            layoutId={`project-image-container-${id}`}\n          >\n            <Link to=\"/\" onClick={() => handleClick()}>\n            <div className={`project-thumbnail thumbnail-${id}`}>\n              <i class={`fas ${icon} coverIcon open`}></i>\n              {/* <div className={`bubble bubble-${id}`}></div> */}\n            </div>\n              {/* <img\n                className=\"project-image\"\n                src={`https://github.com/aarondig/factCheck/blob/main/assets/banner.png?raw=true`}\n                alt=\"\"\n              /> */}\n            </Link>\n           \n          </motion.div>\n          <motion.div\n            className=\"title-container\"\n            layoutId={`title-container-${id}`}\n          >\n            <span className=\"category\">{category}</span>\n            <h2 className=\"projectTitle\">{title}</h2>\n          </motion.div>\n          <motion.div className=\"content-container\" animate>\n            <Component />\n          </motion.div>\n        </motion.div>\n      </div>\n    </>\n  );\n}\n","/Users/Aaron/Desktop/aaronDiggdon/src/components/TravelPins/index.js",["173","174","175","176","177","178","179","180"],"import React, { useState, useRef, useEffect } from \"react\";\nimport \"./style.css\";\nimport { motion, useViewportScroll } from \"framer-motion\";\nimport GetStarted from \"../../img/getStarted.gif\";\nimport NewLogin from \"../../img/newLogin.gif\";\n\nfunction Project() {\n  return (\n    <div id=\"TravelPins\">\n      <a className=\"demoButton\" href=\"https://travel-pins.herokuapp.com\">\n        <div className=\"buttonTextHidden\">DEMO</div>\n        <div className=\"buttonContent\">\n          <div className=\"buttonText\">DEMO</div>\n        </div>\n      </a>\n      <div className=\"projectWrap\">\n        <div className=\"labelC\">\n          <h1 className=\"label\">DESCRIPTION</h1>\n        </div>\n        <div className=\"textC\">\n          <p className=\"description\">\n            A social media application for world travelers that allows users to\n            post photos, mark countries they've traveled to, and follow friends.\n            </p>\n            <p className=\"description\">\n            This project was conducted with two other team members. \n            </p>\n            <p className=\"description\">\n            Role: Frontend Developer/Designer\n            </p>\n        </div>\n\n        <div className=\"techUsed\">\n          {/* <h2 className=\"techUsedHead\">TECH USED</h2> */}\n          <div className=\"listCol\">\n            <div className=\"row\">\n              <div className=\"icon\">\n                <i class=\"fab fa-react navicon\" />\n              </div>\n              <div className=\"column\">\n                <p className=\"listName\">REACT</p>\n                {/* <p>HTML</p> */}\n              </div>\n            </div>\n            <div className=\"row listRow\">\n              <div className=\"icon\">\n                <i class=\"fab fa-js navicon\" />\n              </div>\n              <div className=\"column\">\n                <p className=\"listName\">JAVASCRIPT</p>\n              </div>\n            </div>\n            <div className=\"row listRow\">\n              <div className=\"icon\">\n                <i class=\"fab fa-css3-alt navicon\"></i>\n              </div>\n              <div className=\"column\">\n                <p className=\"listName\">CSS</p>\n              </div>\n            </div>\n          </div>\n          <div className=\"listExplain\">\n            <div className=\"row\">\n              <div className=\"column\">\n                <p className=\"pTitle\">Frontend Dependencies</p>\n                <a className=\"pListLink\" href=\"https://reactjs.org\">\n                  React.js\n                </a>\n                <a\n                  className=\"pListLink\"\n                  href=\"https://docs.mapbox.com/help/glossary/mapbox-gl/\"\n                >\n                  Mapbox-GL\n                </a>\n                <a className=\"pListLink\" href=\"https://www.react-spring.io\">\n                  React-Spring\n                </a>\n\n                <a\n                  className=\"pListLink\"\n                  href=\"https://react-icons.github.io/react-icons/\"\n                >\n                  React-Icons\n                </a>\n                <a\n                  className=\"pListLink\"\n                  href=\"https://www.npmjs.com/package/react-dom\"\n                >\n                  React-Dom\n                </a>\n                <a\n                  className=\"pListLink\"\n                  href=\"https://www.npmjs.com/package/web-vitals\"\n                >\n                  Web-Vitals\n                </a>\n                <a\n                  className=\"pListLink\"\n                  href=\"https://www.npmjs.com/package/re-carousel\"\n                >\n                  Re-Carousel\n                </a>\n                <a\n                  className=\"pListLink\"\n                  href=\"https://www.npmjs.com/package/react-scripts\"\n                >\n                  React-Scripts\n                </a>\n                <a\n                  className=\"pListLink\"\n                  href=\"https://www.npmjs.com/package/react-swipeable\"\n                >\n                  React-Swipeable\n                </a>\n                <a\n                  className=\"pListLink\"\n                  href=\"https://reactrouter.com/web/guides/quick-start\"\n                >\n                  React-Router-Dom\n                </a>\n                <a className=\"pListLink\" href=\"react-transition-group\">\n                  React-Transition-Group\n                </a>\n              </div>\n              <div className=\"column\">\n                <p className=\"pTitle\">Backend Dependencies</p>\n                <a\n                  className=\"pListLink\"\n                  href=\"https://www.npmjs.com/package/axios\"\n                >\n                  Axios\n                </a>\n                <a\n                  className=\"pListLink\"\n                  href=\"https://www.npmjs.com/package/bcrypt\"\n                >\n                  Bcrypt.js\n                </a>\n                <a\n                  className=\"pListLink\"\n                  href=\"https://www.npmjs.com/package/mysql2\"\n                >\n                  MySQL2\n                </a>\n                <a className=\"pListLink\" href=\"https://expressjs.com\">\n                  Express.js\n                </a>\n                <a className=\"pListLink\" href=\"https://cloudinary.com\">\n                  Cloudinary\n                </a>\n                <a className=\"pListLink\" href=\"http://www.passportjs.org\">\n                  Passport.js\n                </a>\n                <a\n                  className=\"pListLink\"\n                  href=\"https://www.npmjs.com/package/passport-local\"\n                >\n                  Passport-Local\n                </a>\n                <a className=\"pListLink\" href=\"https://sequelize.org\">\n                  Sequelize ORM\n                </a>\n                <a\n                  className=\"pListLink\"\n                  href=\"https://www.npmjs.com/package/express-session\"\n                >\n                  Express-Session\n                </a>\n                <a\n                  className=\"pListLink\"\n                  href=\"https://www.npmjs.com/package/if-env\"\n                >\n                  if-env\n                </a>\n              </div>\n            </div>\n          </div>\n        </div>\n        <div className=\"labelC\">\n          <h1 className=\"label\">Features</h1>\n        </div>\n        <div className=\"textC\">\n          <p className=\"description\">\n            Do you love to travel? This app is for you. In this app, you create\n            your own personalized account. You can view all of the countries in\n            the world with a click. If you have traveled to a country, you can\n            mark the country as \"traveled.\" The country will highlight blue to\n            show that you have traveled to the country.\n          </p>\n          <p className=\"description\">\n            You can upload your photos for each country as well! You simply\n            click on the country you want to upload a photo to and then press\n            the plus button to upload. This is an opportunity to keep a photo\n            journal of your travels and share your pictures with others.\n          </p>\n          <p className=\"description\">\n            You can find other travelers as well on the app. You can search\n            their username and follow them. Once you follow them, you can see\n            photos about their travels on your newsfeed.\n          </p>\n          <p className=\"description\">\n            Lastly, you have your own profile account. You can see how many\n            countries you have been to, how many followers you have, how many\n            users follow you, and your photos.\n          </p>\n          <h1 className=\"label\">User Stories</h1>\n          <h2>Get Started</h2>\n          <div className=\"imgContainer\">\n            <img\n              className=\"descriptionImg\"\n              src={\n                GetStarted\n              }\n            ></img>\n            </div>\n            <h2>Create an account or login.</h2>\n            <div className=\"imgContainer\">\n            <img\n              className=\"descriptionImg\"\n              src={NewLogin}\n            ></img>\n          </div>\n\n          <h2>Find a country that has been visited.</h2>\n            <div className=\"imgContainer\">\n            <img\n              className=\"descriptionImg\"\n              src={\"https://github.com/anusontarangkul/travel-pins/blob/main/images/findCountry.gif?raw=true\"}\n            ></img>\n          </div>\n\n         \n        </div>\n      </div>\n    </div>\n  );\n}\n\nexport default Project;\n","/Users/Aaron/Desktop/aaronDiggdon/src/components/Scene/mirrorsData.js",[],"/Users/Aaron/Desktop/aaronDiggdon/src/components/FactCheck/index.js",["181","182","183","184","185","186"],"import React, { useState, useRef, useEffect } from \"react\";\nimport \"./style.css\";\n\nfunction Project() {\n  return (\n    <div id=\"project\">\n       <a\n              className=\"demoButton\"\n              href=\"https://googlefactchecker.herokuapp.com\"\n            >\n              <div className=\"buttonTextHidden\">DEMO</div>\n              <div className=\"buttonContent\">\n                \n                <div className=\"buttonText\">DEMO</div>\n              </div>\n            </a>\n      <div className=\"projectWrap\">\n        <div className=\"labelC\">\n          <h1 className=\"label\">DESCRIPTION</h1>\n        </div>\n        <div className=\"textC\">\n          <p className=\"description\">\n            An application that allows you to search claims, terms and quotes,\n            and returns fact checked articles reguarding the validitiy of the\n            query.\n          </p>\n        </div>\n\n        <div className=\"techUsed\">\n          {/* <h2 className=\"techUsedHead\">TECH USED</h2> */}\n          <div className=\"listCol\">\n            <div className=\"row\">\n              <div className=\"icon\">\n                <i class=\"fab fa-html5 navicon\" />\n              </div>\n              <div className=\"column\">\n                <p className=\"listName\">HTML</p>\n                {/* <p>HTML</p> */}\n              </div>\n            </div>\n            <div className=\"row listRow\">\n              <div className=\"icon\">\n                <i class=\"fab fa-js navicon\" />\n              </div>\n              <div className=\"column\">\n                <p className=\"listName\">JAVASCRIPT</p>\n              </div>\n            </div>\n            <div className=\"row listRow\">\n              <div className=\"icon\">\n              <i class=\"fab fa-css3-alt navicon\"></i>\n              </div>\n              <div className=\"column\">\n                <p className=\"listName\">CSS</p>\n              </div>\n            </div>\n          </div>\n          <div className=\"listExplain\">\n            <p>This was valuble bc of this this and this</p>\n          </div>\n        </div>\n        <div className=\"labelC\">\n          <h1 className=\"label\">Features</h1>\n        </div>\n        <div className=\"textC\">\n          <h2>Search</h2>\n          <p className=\"description\">\n            Using Google's <a href=\"https://developers.google.com/fact-check/tools/api\">Fact Check Tools API</a>, I created a user friendly interface\n            that allows users to easily enter claims and recieve their articles.\n            We used database archetecture with MySQL to save claims, with hopes\n            of using the most-searched-for claims in a featured secton. The\n            application was designed and developed using HTML, CSS and\n            Javascript's JQuery.\n          </p>\n          <h2>User Experience</h2>\n          <p className=\"description\">\n            Creating a seamless user experience was my top priority. I tried to\n            keep a minimalist aesthetic with aesthetic transitions to seamlessly\n            move the user from one action to another.\n          </p>\n          <h2>Routes</h2>\n          <p className=\"description\">\n            Search queries are recieved in the public directory, and passed into\n            the api-routes.js file where the API is called, using axios.js, with\n            the query name. All data is passed using express.js.\n          </p>\n          <div className=\"imgsContainer\">\n            <p>RECIEVED IN PUBLIC</p>\n            <div className=\"imgContainer\">\n              <img\n                className=\"descriptionImg\"\n                src={\n                  \"https://github.com/aarondig/factCheck/blob/main/assets/recieve.png?raw=true\"\n                }\n              ></img>\n            </div>\n            <p className=\"description\">\n              Search queries are recieved in the public directory, and passed\n              into the api-routes.js file where the API is called, using\n              axios.js, with the query name. All data is passed using\n              express.js.\n            </p>\n            <p>API-ROUTES</p>\n            <div className=\"imgContainer\">\n              <img\n                className=\"descriptionImg\"\n                src={\n                  \"https://github.com/aarondig/factCheck/blob/main/assets/passed.png?raw=true\"\n                }\n              ></img>\n            </div>\n          </div>\n          <h2>Models</h2>\n          <p className=\"description\">\n            Using express.js, and sequelize.js I created models for each type of\n            data returned, so I could create HTML elements in Javascript with\n            each specific data type.\n          </p>\n          <div className=\"imgContainer\">\n            <img\n              className=\"descriptionImg\"\n              src={\n                \"https://github.com/aarondig/factCheck/blob/main/assets/models.png?raw=true\"\n              }\n            ></img>\n          </div>\n          <p className=\"description\">\n            Data from the API is passed into the models file from api-routes,\n            using express.js. From there, they are stored in the MySQL database\n            using Sequelize.\n          </p>\n        </div>\n      </div>\n\n      \n    </div>\n  );\n}\n\nexport default Project;\n","/Users/Aaron/Desktop/aaronDiggd