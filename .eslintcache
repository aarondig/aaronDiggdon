          )}\n          </div>\n        )}\n\n        {/* <div className=\"flex-grid\">\n        <div className=\"col\">\n          <ul>\n            <li className=\"item\">HTML</li>\n            <li className=\"item\">CSS</li>\n            <li className=\"item\">JAVASCRIPT</li>\n          </ul>\n        </div>\n        <div className=\"col\">\n       \n        </div>\n        \n      </div> */}\n\n        {/* <div className=\"grid\">\n        <div className=\"gridBanner\">\n          <div className=\"aboutBackground\" id=\"who1\">WHO AM I</div>\n            <div className=\"container\">\n            <h1 className=\"header\">A Freelance Web Designer.</h1>\n          </div>\n        </div>\n        <div className=\"gridLeft\">\n          <div className=\"blah\">hello</div>\n        </div>\n        <div className=\"gridRight\">\n          <div className=\"blah\">hello</div>\n        </div>\n      </div> */}\n      </div>\n    </div>\n  );\n}\n\nexport default About;\n",["163","164"],"/Users/Aaron/Desktop/aaronDiggdon/src/hooks/scrollLock.js",["165"],"import { useEffect, useLayoutEffect, useState } from \"react\";\n\nexport default function useScrollLock() {\n\nfunction status() {\n  \n  function unlocked() {\n    document.body.style.overflow = \"scroll\";\n\n  }\n  function locked() {\n    document.body.style.overflow = \"hidden\";\n\n  }\n  return {\n    lock: () => locked(),\n    unlock: () => unlocked(),\n  }\n}\n\nconst [scrollLock, setScrollLock] = useState(status())\n\nuseEffect(()=> {\n  setScrollLock(status())\n},[])\n\nreturn scrollLock;\n}","/Users/Aaron/Desktop/aaronDiggdon/src/components/Card/data.js",[],"/Users/Aaron/Desktop/aaronDiggdon/src/components/Card/List.js",["166","167","168"],"/Users/Aaron/Desktop/aaronDiggdon/src/components/Card/index.js",["169","170"],"import React, { useState, useEffect } from \"react\";\nimport { AnimateSharedLayout, AnimatePresence } from \"framer-motion\";\n\nimport { Item } from \"./Item\";\nimport { List } from \"./List\